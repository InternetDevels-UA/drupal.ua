/**
 * @file
 * Install, uninstall functions for the og_to_forum module.
 */


/**
* Implements hook_install().
*/
function og_to_forum_install() {
  $status = array(
    'step' => 0,  //Create vocabulary
  );
  variable_set('og_to_forum_status', $status);
}


/**
* Implements hook_uninstall().
*/
function og_to_forum_uninstall() {
  variable_del('og_to_forum_status');
}

